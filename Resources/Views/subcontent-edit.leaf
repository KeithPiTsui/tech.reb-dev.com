<!DOCTYPE html>
<html>

<head>
  <title>SubContent-Edit</title>
  #embed("embeds/resource")
</head>

<body>
  #embed("logined-header")

  <div class="content">

    <a class="btn btn-default" href="/subcontents/new">コンテンツを追加</a>

    #loop(subcontents, "subcontent") {
      <div class="card">
        <div class="card-block">
          <h3 class="card-title">#(subcontent.name)</h3>
          <p class="card-text">#(subcontent.part_of_content)</p>
          <a href="/subcontents/update/#(subcontent.id)" class="btn btn-default">編集する</a>
        </div>
      </div>
    }

    <div class="clearbox"></div>
  </div>

  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js"></script>
</body>

</html>
